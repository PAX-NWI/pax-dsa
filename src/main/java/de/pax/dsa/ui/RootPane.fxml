<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.pax.dsa.ui.RootPaneController">
   <top>
      <HBox spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#handleLogin" text="Login" />
            <Button mnemonicParsing="false" onAction="#handleAddElement" text="Add Element" />
            <Button fx:id="doMovesButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Do Moves" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <Pane fx:id="mapPane" prefHeight="800.0" prefWidth="1000.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
